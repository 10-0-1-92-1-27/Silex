{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "silex": true,
    "goog": true,
    "ga": true,
    "alertify": true,
    "ce": true,
    "Pixlr": true,
    "ace": true
  },
  "rules": {
    "quotes": [2, "single"],
    "no-unused-vars": 1,
    "no-loop-func": 1,
    "no-underscore-dangle": 0,
    "strict": 0,
    "camelcase": 0,
    "no-console": 0,
    "dot-notation": 0
  }
}